{
  "name": "debjit/php-ai-hub",
  "description": "Portable AI Hub for Laravel. No vendor, no hassle. Just drop in and use.",
  "type": "composer-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "AI HTTP Service Installer",
      "email": "hi@debjit.in"
    }
  ],
  "require": {
    "php": "^8.2 || ^8.3",
    "composer-plugin-api": "^2.2",
    "symfony/filesystem": "^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "PhpAiHub\\Package\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpAiHub\\Base\\Tests\\": "tests/"
    }
  },
  "extra": {
    "class": "PhpAiHub\\Package\\Composer\\AIHubPlugin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": ["ai", "sdk", "no-sdk", "laravel", "composer-plugin"]
}
